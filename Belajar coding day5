import java.util.*;

 // Compiler version JDK 11.0.2

 class Dcoder
 {
   public static void main(String args[])
   { 
    int a = 14, b = 8;
    
    int jumlah = a+b;
    int kurang = a-b;
    
    System.out.println ("penjumlahan :"+jumlah);
    System.out.println ("pengurangan :"+ kurang);
   }
 }
